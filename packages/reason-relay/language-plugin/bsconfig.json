{
  "name": "relay-compiler-language-reason",
  "version": "0.1.0",
  "sources": {
    "dir" : "src",
    "subdirs" : true
  },
  "package-specs": {
    "module": "commonjs",
    "in-source": true
  },
  "suffix": ".bs.js",
  "bs-dependencies": [
    "bs-flow-parser",
    "tablecloth-bucklescript"
  ],
  "warnings": {
    "error" : "+101"
  },
  "refmt": 3,
  "gentypeconfig": {
    "language": "typescript",
    "shims": {
      "Js": "Js"
    },
    "debug": {
      "all": false,
      "basic": false
    }
  }
}
